<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapns">
	<select id="selectStation" parameterType="String"
		resultType="com.mycompany.myapp.model.stationXY">
		SELECT * FROM SUB_RCM WHERE SUBNAME = #{subname}
	</select>
	<insert id="insertData" parameterType="route">
		insert into route values (#{id},#{departure},#{bus_route},#{complex_route},#{bus_time, jdbcType=VARCHAR},#{complex_time, jdbcType=VARCHAR})
	</insert>
	<select id="routeSearch" parameterType="String" resultType="com.mycompany.myapp.model.Route">
		select * from route where id=#{id}
	</select>
	<select id="idCheck" parameterType="String" resultType="com.mycompany.myapp.model.Route">
		select * from route where id=#{id}
	</select>
</mapper>






















